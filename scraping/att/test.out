				return unescape(document.cookie.substring(c_start,c_end)); 
			} 
		} 
		return ""; 
	}  
	function updateVESessionCookie(){  
		var veCookieName = "VirtuOz-attve-session"; 
		var chatCookieName = "VirtuOz-attchat-session"; 
		var path="/"; 
		var domain=".att.com"; 
		var secure=false;   

		var veCookieVal = getCookie(veCookieName); 
		var chatCookieVal = getCookie(chatCookieName);   

		if((veCookieVal != undefined) && (veCookieVal.length > 0)){
			veCookieVal = decodeURIComponent(veCookieVal); 
			veCookieVal = veCookieVal.replace(/@[^@"]+@[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}/g,""); 
			veCookieVal = encodeURIComponent(veCookieVal); 
			setVESessionCookie(veCookieName,veCookieVal,path,domain,secure);
		}  

		if((chatCookieVal != undefined) && (chatCookieVal.length > 0)){
			chatCookieVal = decodeURIComponent(chatCookieVal); 
			chatCookieVal = chatCookieVal.replace(/@[^@"]+@[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}/g,""); 
			chatCookieVal = encodeURIComponent(chatCookieVal);
			setVESessionCookie(chatCookieName,chatCookieVal,path,domain,secure);
		}	 
	} 

	function getTopLevelDomain(domain){
		var domainVars = domain.split("\.");
		var topLevelDomainLast = domainVars.pop();
		var topLevelDomainFirst = domainVars.pop();

		return "." + topLevelDomainFirst + "." + topLevelDomainLast;
	}
	 
	function setVESessionCookie(name, value, path, domain, secure) {

		var curCookie = name + "=" + value  
		+ ((path) ? "; path=" + path : "") 
		+ ((domain) ? "; domain=" + getTopLevelDomain(domain) : "") 
		+ ((secure) ? "; secure" : "");

		document.cookie = curCookie; 
	}
	</script>


